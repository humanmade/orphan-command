{
  "name": "humanmade/orphan-command",
  "description": "List and delete orphan WordPress entities and metadata.",
  "type": "wp-cli-package",
  "homepage": "https://github.com/humanmade/orphan-command",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Human Made",
      "homepage": "https://humanmade.com"
    }
  ],
  "support": {
    "issues": "https://github.com/humanmade/orphan-command/issues"
  },
  "require": {
    "php": ">=7.2",
    "composer/installers": "^1.0",
    "wp-cli/wp-cli": "^2.5"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "humanmade/coding-standards": "^1.1",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "HumanMade\\OrphanCommand\\": "inc/"
    },
    "files": [ "orphan-command.php" ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "lint": "find ./orphan-command.php ./inc/ -type f -name '*.php' -print0 | xargs -0 -L 1 -P 4 -- php -l",
    "phpcs": "phpcs .",
    "test": [
      "@lint",
      "@phpcs"
    ]
  },
  "extra": {
    "commands": [
      "orphan blogmeta",
      "orphan comment",
      "orphan commentmeta",
      "orphan post",
      "orphan postmeta",
      "orphan termmeta",
      "orphan usermeta"
    ]
  }
}
